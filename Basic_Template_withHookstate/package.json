{
    "name": "react-typescript-boilerplate",
    "version": "1.0.0",
    "private": true,
    "description": "A frontend boilerplate with React and Typescript.",
    "homepage": "",
    "keywords": [
        "react",
        "typescript",
        "boilerplate"
    ],
    "main": "index.js",
    "scripts": {
        "predeploy": "npm run build",
        "start": "webpack serve --mode development --hot --progress --port 3000",
        "build": "webpack && npm run CopyExternalConfigToBuild && npm run AdaptForProduction",
        "CopyExternalConfigToBuild": "xcopy /Y src\\externalConfig.prod.json build\\externalConfig.json*",
        "AdaptForProduction": "node scripts/adaptForproduction.js / true",
        "prettier": "prettier --write \"src/**/*.{ts,tsx,css}\"",
        "lintfix": "npx eslint  src/**/**/* --fix",
        "lint": "npx eslint  src/**/**/* --fix-dry-run",
        "test": "jest",
        "test:watch": "jest --watch"
    },
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "^7.12.7",
        "@types/enzyme": "^3.10.8",
        "@types/enzyme-adapter-react-16": "^1.0.5",
        "@types/jest": "^26.0.15",
        "@types/node": "^14.14.9",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "@types/webpack": "^4.41.25",
        "@types/webpack-env": "1.15.3",
        "@typescript-eslint/eslint-plugin": "^4.8.1",
        "@typescript-eslint/parser": "^4.8.1",
        "babel-loader": "^8.2.1",
        "clean-webpack-plugin": "^3.0.0",
        "css-loader": "^5.0.1",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.5",
        "enzyme-to-json": "^3.6.1",
        "eslint": "^7.14.0",
        "eslint-config-prettier": "^6.15.0",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-react": "^7.21.5",
        "file-loader": "^6.2.0",
        "html-loader": "^1.3.2",
        "html-webpack-plugin": "^4.5.0",
        "jest": "^26.6.3",
        "jest-css-modules": "^2.1.0",
        "mini-css-extract-plugin": "^1.3.1",
        "optimize-css-assets-webpack-plugin": "^5.0.4",
        "postcss": "^8.1.9",
        "postcss-browser-reporter": "^0.6.0",
        "postcss-import": "^13.0.0",
        "postcss-loader": "^4.1.0",
        "postcss-preset-env": "^6.7.0",
        "postcss-reporter": "^7.0.1",
        "postcss-url": "^10.1.0",
        "prettier": "^2.2.0",
        "react-hot-loader": "^4.13.0",
        "style-loader": "^2.0.0",
        "ts-jest": "^26.4.4",
        "ts-loader": "^8.0.11",
        "typescript": "^4.1.2",
        "url-loader": "^4.1.1",
        "webpack": "5.6.0",
        "webpack-cli": "^4.2.0",
        "webpack-dev-server": "^3.11.0"
    },
    "dependencies": {
        "@hookstate/core": "^3.0.1",
        "react": "^17.0.1",
        "react-dom": "^17.0.1"
    }
}
