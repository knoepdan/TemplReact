{
    "name": "react-typescript-boilerplate",
    "version": "1.0.0",
    "private": true,
    "description": "A frontend boilerplate with React and Typescript.",
    "homepage": "",
    "keywords": [
        "react",
        "typescript",
        "boilerplate"
    ],
    "main": "index.js",
    "scripts": {
        "predeploy": "npm run build",
        "start": "webpack-dev-server --mode development --hot --progress --color --port 3000 --open",
        "build": "webpack -p --progress --colors && npm run CopyExternalConfigToBuild",
        "CopyExternalConfigToBuild": "xcopy /Y src\\externalConfig.prod.json build\\externalConfig.json*",
        "prettier": "prettier --write \"src/**/*.{ts,tsx,css}\"",
        "lintfix": "npx eslint  src/**/**/* --fix",
        "lint": "npx eslint  src/**/**/* --fix-dry-run",
        "test": "jest",
        "test:watch": "jest --watch"
    },
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "^7.9.6",
        "@types/enzyme": "^3.10.4",
        "@types/enzyme-adapter-react-16": "^1.0.5",
        "@types/jest": "^25.2.1",
        "@types/node": "^13.13.4",
        "@types/react": "^16.9.34",
        "@types/react-dom": "^16.9.7",
        "@types/react-router-dom": "^5.1.5",
        "@types/webpack": "^4.41.12",
        "@types/webpack-env": "1.15.1",
        "@typescript-eslint/eslint-plugin": "^2.30.0",
        "@typescript-eslint/parser": "^2.30.0",
        "babel-loader": "^8.1.0",
        "clean-webpack-plugin": "^3.0.0",
        "css-loader": "^3.5.3",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.2",
        "enzyme-to-json": "^3.4.3",
        "eslint": "^6.7.2",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.1",
        "eslint-plugin-react": "^7.17.0",
        "file-loader": "^6.0.0",
        "html-loader": "^1.1.0",
        "html-webpack-plugin": "^4.3.0",
        "jest": "^25.5.3",
        "jest-css-modules": "^2.1.0",
        "mini-css-extract-plugin": "^0.9.0",
        "optimize-css-assets-webpack-plugin": "^5.0.3",
        "postcss": "^7.0.27",
        "postcss-browser-reporter": "^0.6.0",
        "postcss-import": "^12.0.1",
        "postcss-loader": "^3.0.0",
        "postcss-preset-env": "^6.7.0",
        "postcss-reporter": "^6.0.1",
        "postcss-url": "^8.0.0",
        "prettier": "^2.0.5",
        "react-hot-loader": "^4.12.21",
        "style-loader": "^1.2.1",
        "ts-jest": "^25.4.0",
        "ts-loader": "^7.0.2",
        "typescript": "^3.8.3",
        "url-loader": "^4.1.0",
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.11",
        "webpack-dev-server": "^3.10.3"
    },
    "dependencies": {
        "@hookstate/core": "^1.7.1",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-router-dom": "^5.1.2"
    }
}
